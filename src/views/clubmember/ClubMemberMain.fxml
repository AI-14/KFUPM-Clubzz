<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="700.0" stylesheets="@../../styles/clubmember/ClubMemberMain.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.clubmember.ClubMemberMainController">
   <children>
      <Button fx:id="LeaveClubBtn" layoutX="408.0" layoutY="334.0" mnemonicParsing="false" onAction="#LeaveClub" prefHeight="54.0" prefWidth="250.0" text="LEAVE SELECTED CLUB">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Button>
      <Button fx:id="ViewProjectBtn" layoutX="52.0" layoutY="334.0" mnemonicParsing="false" onAction="#viewProject" prefHeight="54.0" prefWidth="250.0" text="VIEW PROJECTS">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Button>
      <ComboBox fx:id="ClubComboBox" layoutX="236.0" layoutY="228.0" prefHeight="65.0" prefWidth="250.0" promptText="SELECT FROM AVAILABLE CLUBS" />
      <Button fx:id="SignOutBtn" layoutX="623.0" layoutY="14.0" mnemonicParsing="false" onAction="#SignOut" text="Sign Out">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Label fx:id="msgLabel" layoutX="246.0" layoutY="423.0" prefHeight="46.0" prefWidth="230.0" textAlignment="CENTER" />
      <ImageView fitHeight="203.0" fitWidth="224.0" layoutX="258.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/clubmembericon.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
